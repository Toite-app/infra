# Traefik / Domain configuration
# DOMAIN: the domain name for your app (e.g., demo.toite.ee)
# LETSENCRYPT_EMAIL: email for Let's Encrypt certificate notifications
# DOCKER_SOCK: path to Docker socket (for rootless Docker use /run/user/<uid>/docker.sock)
DOMAIN=localhost
LETSENCRYPT_EMAIL=
DOCKER_SOCK=/var/run/docker.sock

# Database passwords (for docker-compose)
MONGO_PASSWORD=
POSTGRES_PASSWORD=
REDIS_PASSWORD=

# App config
JWT_SECRET=
SESSION_EXPIRES_IN=4h
INITIAL_ADMIN_PASSWORD=
COOKIES_SECRET=
CSRF_SECRET=

# External APIs (optional)
DADATA_API_TOKEN=
GOOGLE_MAPS_API_KEY=

# S3 / MinIO (defaults work with local MinIO)
S3_ACCESS_KEY_ID=minioadmin
S3_SECRET_ACCESS_KEY=minioadmin
S3_BUCKET_NAME=toite
S3_ENDPOINT=http://localhost:9000
S3_REGION=us-east-1
